
var userName = "Ethan";
console.log(`Hello ${userName}!`);
var userQuestion = "What is my future ?"
console.log(`${userName} asked:"${userQuestion}"`);
var ramdomNumber = Math.floor(Math.random()*8);
var eightBall = "";
switch(ramdomNumber){
    case 0:
        eightBall = 'It is certain';
        break;
    case 1:
        eightBall = 'It is decidedly so';
        break;
    case 2:
        eightBall = 'Reply hazy try again';
        break;
    case 3:
        eightBall = 'Cannot predict right now';
        break;
    case 4:
        eightBall = 'Do not count on it';
        break; 
    case 5:
        eightBall = 'My source say no';
        break;  
    case 6:
        eightBall = 'Outlook not so good';
        break;
    case 7:
        eightBall = 'Sign point to yes';
        break;
}
console.log(eightBall);
